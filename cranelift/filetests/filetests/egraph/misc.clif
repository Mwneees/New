test optimize precise-output
set opt_level=speed
target x86_64

function %stack_load(i64) -> i64 {
  ss0 = explicit_slot 8

block0(v0: i64):
  stack_store.i64 v0, ss0
  v1 = stack_load.i64 ss0
  return v1
}

